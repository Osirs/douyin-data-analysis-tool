{
  "name": "douyin-analytics-tool",
  "version": "1.0.0",
  "description": "抖音数据推送及分析工具 - 基于MySQL数据库的企业级解决方案",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "mysql -u root -p < config/init.sql",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo \"No build process required for this project\"",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "douyin",
    "tiktok",
    "analytics",
    "data",
    "mysql",
    "nodejs",
    "express",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/douyin-analytics-tool.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/douyin-analytics-tool/issues"
  },
  "homepage": "https://github.com/yourusername/douyin-analytics-tool#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "*.log"
    ],
    "delay": "1000"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}